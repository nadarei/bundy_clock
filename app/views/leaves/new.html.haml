- section :leave
%section.leave

  %h3 File leave

  = form_for @time_log, url: leaves_path do |f|
    %fieldset.date
      %h4 Effective date:
      = f.text_field :date, size: 11, id: 'datepicker'
      %br

      = render partial: 'leave_type', locals: { time_log: @time_log }

    %fieldset.actions
      = f.submit "Submit"
